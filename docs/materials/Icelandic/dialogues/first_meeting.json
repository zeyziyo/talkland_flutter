  "entries": [
    {
      "text": "Halló! Ég heiti Jordan. Gaman að kynnast þér.",
      "note": "",
      "speaker": "Jordan",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "Halló Jordan! Ég er Minji. Gaman að kynnast þér líka.",
      "note": "",
      "speaker": "Minji",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "Ertu nemandi?",
      "note": "",
      "speaker": "Jordan",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "Nei, ég er skrifstofumaður. Hvað með þig?",
      "note": "",
      "speaker": "Minji",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "Ég er háskólanemi. Ég er að læra kóresku.",
      "note": "",
      "speaker": "Jordan",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "Vá, það er frábært! Kóreska er svolítið erfið, er það ekki?",
      "note": "",
      "speaker": "Minji",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "Já, hún er erfið en skemmtileg. Vinsamlegast hjálpaðu mér mikið.",
      "note": "",
      "speaker": "Jordan",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "Auðvitað! Lærum hart saman.",
      "note": "",
      "speaker": "Minji",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "Takk fyrir. Sjáumst næst!",
      "note": "",
      "speaker": "Jordan",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "Bless!",
      "note": "",
      "speaker": "Minji",
      "form_type": "",
