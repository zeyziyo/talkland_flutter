      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "Salam Jordan! Mən Minjiyəm. Mən də səninlə tanış olmağıma çox şadam.",
      "note": "",
      "speaker": "Minji",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "Sən tələbəsən?",
      "note": "",
