  "entries": [
    {
      "text": "Kaixo! Jordan dut izena. Pozten nau zu ezagutzeak.",
      "note": "",
      "speaker": "Jordan",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "Kaixo Jordan! Minji naiz. Ni ere pozten naiz zu ezagutzeaz.",
      "note": "",
      "speaker": "Minji",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "Ikaslea zara?",
      "note": "",
      "speaker": "Jordan",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "Ez, bulegari bat naiz. Eta zu?",
      "note": "",
      "speaker": "Minji",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "Unibertsitateko ikaslea naiz. Koreera ikasten ari naiz.",
      "note": "",
      "speaker": "Jordan",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "A, zeinen ona! Koreera zaila da, ezta?",
      "note": "",
      "speaker": "Minji",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "Bai, zaila da baina dibertigarria. Mesedez, lagundu iezadazu asko.",
      "note": "",
      "speaker": "Jordan",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "Noski! Ikas dezagun gogor elkarrekin.",
      "note": "",
      "speaker": "Minji",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "Eskerrik asko. Hurrengoan arte!",
      "note": "",
      "speaker": "Jordan",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "Agur!",
      "note": "",
      "speaker": "Minji",
      "form_type": "",
      "style": "",
      "tags": []
    }
  ]
}