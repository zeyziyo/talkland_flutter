  "entries": [
    {
      "text": "Sveiki! Mani sauc Džordans. Prieks iepazīties.",
      "note": "",
      "speaker": "Jordan",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "Sveiks, Džordan! Esmu Mindži. Arī man prieks iepazīties.",
      "note": "",
      "speaker": "Minji",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "Vai tu esi students?",
      "note": "",
      "speaker": "Jordan",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "Nē, esmu biroja darbinieks. Bet tu?",
      "note": "",
      "speaker": "Minji",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "Esmu koledžas students. Es studēju korejiešu valodu.",
      "note": "",
      "speaker": "Jordan",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "Oho, tas ir lieliski! Korejiešu valoda ir nedaudz grūta, vai ne?",
      "note": "",
      "speaker": "Minji",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "Jā, tā ir grūta, bet jautra. Lūdzu, palīdzi man daudz.",
      "note": "",
      "speaker": "Jordan",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "Protams! Mācīsimies kopā.",
      "note": "",
      "speaker": "Minji",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "Paldies. Uz redzēšanos nākamreiz!",
      "note": "",
      "speaker": "Jordan",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "Atā!",
      "note": "",
      "speaker": "Minji",
      "form_type": "",
      "style": "",
