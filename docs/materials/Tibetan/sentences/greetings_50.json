{
    "subject": "སྐད་རbasic་གཞི་རྩའི་འཚམས་འདྲི་དང་ཉིན་རེའི་ཐ་ཚིག་ ༥༠",
    "entries": [
        {
            "text": "བཀྲ་ཤིས་བདེ་ལེགས།",
            "note": "Polite greeting"
        },
        {
            "text": "ཐུག་པར་དགའ་པོ་བྱུང་།",
            "note": "First meeting"
        },
        {
            "text": "ཡུན་རིང་མ་ཐུག",
            "note": "Reunion"
        },
        {
            "text": "ཐུགས་རྗེ་ཆེ།",
            "note": "Formal"
        },
        {
            "text": "དགོངས་དག",
            "note": "Formal"
        },
        {
            "text": "དགོངས་འགལ་མེད་པར་ཞུ།",
            "note": "Attention"
        },
        {
            "text": "ཁྱེད་རང་སྐུ་གཇི་ལྟར་འདུག",
            "note": "Greetings"
        },
        {
            "text": "ང་བདེ་པོ་ཡོད། ཐུགས་རྗེ་ཆེ།",
            "note": ""
        },
        {
            "text": "ཁྱེད་ཀྱི་མཚན་ལ་ཅི་ཟེར།",
            "note": ""
        },
        {
            "text": "ངའི་མིང་ལ་...ཟེར།",
            "note": ""
        },
        {
            "text": "ཁྱེད་རང་གང་ནས་ཡིན།",
            "note": ""
        },
        {
            "text": "ང་ཨ་མི་རི་ཀ་ནས་ཡིན།",
            "note": ""
        },
        {
            "text": "ཕེབས་དང་།",
            "note": "To someone leaving"
        },
        {
            "text": "བཞུགས་དང་།",
            "note": "To someone staying"
        },
        {
            "text": "ཡོད།",
            "note": "Positive"
        },
        {
            "text": "མེད།",
            "note": "Negative"
        },
        {
            "text": "ཡིན་ཡང་སྲིད།",
            "note": "Guess"
        },
        {
            "text": "ང་ལ་ཧ་གོ་བྱུང་།",
            "note": ""
        },
        {
            "text": "ང་མི་ཤེས།",
            "note": ""
        },
        {
            "text": "ང་ལ་རོགས་རམ་གནང་རོགས།",
            "note": ""
        },
        {
            "text": "སྐད་ཅིག་ཙམ་སྒུག་དང་།",
            "note": ""
        },
        {
            "text": "འདི་ག་ཚོད་རེད།",
            "note": "Shopping"
        },
        {
            "text": "གསང་སྤྱོད་ཁང་གང་དུ་ཡོད།",
            "note": ""
        },
        {
            "text": "ང་ལ་བཀྲེས་སོང་།",
            "note": ""
        },
        {
            "text": "ང་སྐོམ་སོང་།",
            "note": ""
        },
        {
            "text": "ཞིམ་པོ་འདུག",
            "note": ""
        },
        {
            "text": "ང་དགའ་པོ་བྱུང་།",
            "note": ""
        },
        {
            "text": "འགྲིག་གི་འདུག",
            "note": ""
        },
        {
            "text": "བཀྲ་ཤིས་བདེ་ལེགས།",
            "note": ""
        },
        {
            "text": "ད་ལྟ་ཆུ་ཚོད་ག་ཚོད་རེད།",
            "note": ""
        },
        {
            "text": "དེ་རིང་གནམ་གཤིས་ཡག་པོ་འདུག",
            "note": ""
        },
        {
            "text": "གྲང་མོ་འདུག",
            "note": ""
        },
        {
            "text": "ཚ་པོ་འདུག",
            "note": ""
        },
        {
            "text": "ང་ངལ་དུབ་འདུག",
            "note": ""
        },
        {
            "text": "ང་སྤྲོ་སྣང་འདུག",
            "note": ""
        },
        {
            "text": "འཚམས་འདྲི་ཞུ།",
            "note": ""
        },
        {
            "text": "སྐྱེས་སྐར་བཀྲ་ཤིས་བདེ་ལེགས།",
            "note": ""
        },
        {
            "text": "ཉིན་གུང་བདེ་ལེགས།",
            "note": ""
        },
        {
            "text": "སང་ཉིན་ཐུག་གི་ཡིན།",
            "note": ""
        },
        {
            "text": "དོ་གཟབ་གནང་།",
            "note": ""
        },
        {
            "text": "ཁྱེད་རང་གིས་དབྱིན་ཇི་སྐད་བཤད་ཐུབ་བམ།",
            "note": ""
        },
        {
            "text": "ང་ལ་ཀོ་རི་ཡའི་སྐད་ཅུང་ཙམ་མ་གཏོགས་བཤད་མི་ཐུབ།",
            "note": ""
        },
        {
            "text": "ངPlease་བཀྲོལ་ནས་གསུང་རོགས།",
            "note": ""
        },
        {
            "text": "འདིའི་དོན་ག་རེ་རེད།",
            "note": ""
        },
        {
            "text": "ཀོ་རི་ཡའི་སྐད་དུ་ག་རེ་ཟེར།",
            "note": ""
        },
        {
            "text": "ང་ཁྱེད་ལ་དགའ།",
            "note": ""
        },
        {
            "text": "སྒུག་དང་།",
            "note": ""
        },
        {
            "text": "ཕེབས་དང་།",
            "note": ""
        },
        {
            "text": "ལགས་སོ། འགྲིག་གི་འདུག",
            "note": ""
        },
        {
            "text": "ང་ལ་དྲི་བ་ཞིག་ཡོད།",
            "note": ""
        }
    ]
