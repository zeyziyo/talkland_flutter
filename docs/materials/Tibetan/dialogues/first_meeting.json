  "entries": [
    {
      "text": "ཀྱེ་མ། ངའི་མིང་ལ་ཇོར་ཌན་ཟེར། ཁྱེད་རང་ཐུག་པར་དགའ་པོ་བྱུང་།",
      "note": "",
      "speaker": "Jordan",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "ཀྱེ་ཇོར་ཌན། ང་མིན་ཇི་ཡིན། ཁྱེད་རང་ཐུག་པར་དགའ་པོ་བྱུང་།",
      "note": "",
      "speaker": "Minji",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "ཁྱེད་རང་སློབ་མ་ཞིག་ཡིན་ནམ།",
      "note": "",
      "speaker": "Jordan",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "མེད། ང་ནི་ལས་བྱེད་པ་ཞིག་ཡིན། ཁྱེད་རང་ག་རེ་ཡིན།",
      "note": "",
      "speaker": "Minji",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "ང་ནི་མཐོ་སློབ་སློབ་མ་ཞིག་ཡིན། ང་ཀོ་རི་ཡའི་སྐད་ཡིག་སྦྱོང་བཞིན་ཡོད།",
      "note": "",
      "speaker": "Jordan",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "ཡ་མཚན་ཅན་རེད། ཀོ་རི་ཡའི་སྐད་ཡིག་ཅུང་ཁག་པོ་འདུག་གམ།",
      "note": "",
      "speaker": "Minji",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "ཡོད། ཁག་པོ་ཡོད་ལ་སྤྲོ་སྣང་ཡང་ཡོད། ང་ལ་རོགས་རམ་མང་པོ་གནང་རོགས།",
      "note": "",
      "speaker": "Jordan",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "འགྲིག་གི་རེད། ང་ཚོས་མཉམ་དུ་ཧུར་ཐག་བྱེད་དགོས།",
      "note": "",
      "speaker": "Minji",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "ཐུགས་རྗེ་ཆེ། རྗེས་སུ་ཐུག་ཡོང་།",
      "note": "",
      "speaker": "Jordan",
      "form_type": "",
      "style": "",
      "tags": []
    },
    {
      "text": "བདེ་མོ།",
      "note": "",
      "speaker": "Minji",
      "form_type": "",
      "style": "",
      "tags": []
    }
  ]
}